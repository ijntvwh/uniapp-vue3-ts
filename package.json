  "version": "1.0.4",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install && types-sync && echo '{}' > src/manifest.json && echo '{\"pages\": [{\"path\": \"\"}]}' > src/pages.json",
    "uvm": "npx @dcloudio/uvm@latest",
    "postuvm": "bun ./scripts/postupgrade.ts",
    "dev": "uni -p mp-weixin",
    "build": "uni build -p mp-weixin",
    "preview": "bash scripts/preview.sh",
    "lint": "eslint",
    "docker": "docker build -t uni-mp . && docker run -v $(pwd)/dist:/app/dist --rm uni-mp"
  },
  "dependencies": {
